<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.pwnpy" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PwnPy!</name>
    <icon density="mdpi" height="57" src="res/icon.png" width="57" />
    <description>
        Simple app to help you with Python.
    </description>
    <author email="aleksandar.b@vivifyideas.com" href="https://vivifyideas.com">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.1.1" />
</widget>
